set number
syntax enable
colorscheme dracula
set hlsearch

set foldmethod=indent
set foldlevel=1

nnoremap <space> za
nnoremap th  :tabfirst<CR>
nnoremap tj  :tabnext<CR>
nnoremap tk  :tabprev<CR>
nnoremap tl  :tablast<CR>
nnoremap tt  :taedit<Space>
nnoremap tn  :tabnew
nnoremap tm  :tabm<Space>
nnoremap td  :tabclose<CR>
